local v0=game:GetService("Players");
local v1=game:GetService("RunService");
local v2=game:GetService("UserInputService");
local v3=game:GetService("TweenService");
local v4=game:GetService("CoreGui");
local v5=workspace.CurrentCamera;
local v6=v0.LocalPlayer;
local v7={
    ESP={
        Enabled=false,
        BoxColor=Color3.new(1,0,0.3),
        DistanceColor=Color3.new(1,1,1),
        HealthGradient={
            Color3.new(0,1,0),
            Color3.new(1,1,0),
            Color3.new(1,0,0)
        },
        SnaplineEnabled=true,
        SnaplinePosition="Center",
        RainbowEnabled=false
    },
    Aimbot={
        Enabled=false,
        FOV=30,
        MaxDistance=200,
        ShowFOV=false,
        TargetPart="Head",
        WallCheck=true
    }
};
local v8=0.5;
local v9={};

local function v10(v371)
    if v371==v6 then return end
    local v373={
        Box=Drawing.new("Square"),
        HealthBar=Drawing.new("Square"),
        Distance=Drawing.new("Text"),
        Snapline=Drawing.new("Line")
    };
    
    for _,v515 in pairs(v373) do
        v515.Visible=false
        if v515.Type=="Square" then
            v515.Thickness=2
            v515.Filled=false
        end
    end
    
    v373.Box.Color=v7.ESP.BoxColor
    v373.HealthBar.Filled=true
    v373.Distance.Size=16
    v373.Distance.Center=true
    v373.Distance.Color=v7.ESP.DistanceColor
    v373.Snapline.Color=v7.ESP.BoxColor
    v9[v371]=v373
end

local function v11(v374,v375)
    if not v7.ESP.Enabled or not v374.Character then
        for _,v460 in pairs(v375) do v460.Visible=false end
        return
    end

    local v376=v374.Character:FindFirstChildOfClass("Humanoid")
    local v377=v374.Character:FindFirstChild("Head")
    if not v376 or v376.Health <= 0 or not v377 then
        for _,v541 in pairs(v375) do v541.Visible=false end
        return
    end

    -- Check for forcefield
    if v374.Character:FindFirstChildWhichIsA("ForceField") then
        for _,v544 in pairs(v375) do v544.Visible=false end
        return
    end

    local v378,v379=v5:WorldToViewportPoint(v377.Position)
    if not v379 then
        for _,v546 in pairs(v375) do v546.Visible=false end
        return
    end

    local v380=(v377.Position-v5.CFrame.Position).Magnitude
    local v381=1000/v380
    v375.Box.Size=Vector2.new(v381,v381*1.5)
    v375.Box.Position=Vector2.new(v378.X-v381/2,v378.Y-v381*0.75)
    v375.Box.Visible=true

    local v385=v376.Health/v376.MaxHealth
    local v386=math.clamp(3-v385*2,1,3)
    local v387=v7.ESP.HealthGradient[math.floor(v386)]:Lerp(v7.ESP.HealthGradient[math.ceil(v386)],v386%1)
    v375.HealthBar.Size=Vector2.new(4,v381*1.5*v385)
    v375.HealthBar.Position=Vector2.new(v378.X+v381/2+1,v378.Y-v381*0.75+v381*1.5*(1-v385))
    v375.HealthBar.Color=v387
    v375.HealthBar.Visible=true

    v375.Distance.Text=math.floor(v380).."m"
    v375.Distance.Position=Vector2.new(v378.X,v378.Y+v381*0.75+5)
    v375.Distance.Visible=true

    if v7.ESP.RainbowEnabled then
        local v438=(tick()*v8)%1
        v375.Snapline.Color=Color3.fromHSV(v438,1,1)
        v375.Box.Color=Color3.fromHSV(v438,1,1)
    else
        v375.Snapline.Color=v7.ESP.BoxColor
        v375.Box.Color=v7.ESP.BoxColor
    end

    if v7.ESP.SnaplineEnabled then
        local v441
        if v7.ESP.SnaplinePosition=="Bottom" then
            v441=v5.ViewportSize.Y
        elseif v7.ESP.SnaplinePosition=="Top" then
            v441=0
        else
            v441=v5.ViewportSize.Y/2
        end
        
        v375.Snapline.From=Vector2.new(v378.X,v378.Y+v381*0.75)
        v375.Snapline.To=Vector2.new(v5.ViewportSize.X/2,v441)
        v375.Snapline.Visible=true
    else
        v375.Snapline.Visible=false
    end
end

local function v12()
    local targetPlayer=nil
    local closestDistance=math.huge
    local fovRadius=v7.Aimbot.FOV/2
    
    for _,player in pairs(v0:GetPlayers()) do
        if player~=v0.LocalPlayer and player.Character then
            local head=player.Character:FindFirstChild("Head")
            local humanoid=player.Character:FindFirstChildOfClass("Humanoid")
            
            -- Skip if dead, no head, or has forcefield
            if head and humanoid and humanoid.Health>0 and 
               not player.Character:FindFirstChildWhichIsA("ForceField") then
                
                local distance=(head.Position-v5.CFrame.Position).Magnitude
                if distance<=v7.Aimbot.MaxDistance then
                    local direction=(head.Position-v5.CFrame.Position).Unit
                    local lookVector=v5.CFrame.LookVector
                    local angle=math.deg(math.acos(direction:Dot(lookVector)))
                    
                    if angle<=fovRadius then
                        -- Wall check
                        if v7.Aimbot.WallCheck then
                            local origin=v5.CFrame.Position
                            local ray=Ray.new(origin,(head.Position-origin))
                            local params=RaycastParams.new()
                            params.FilterDescendantsInstances={v0.LocalPlayer.Character,player.Character}
                            params.FilterType=Enum.RaycastFilterType.Blacklist
                            local result=workspace:Raycast(ray.Origin,ray.Direction,params)
                            
                            -- Skip if wall is blocking
                            if result then
                                continue
                            end
                        end
                        
                        if distance<closestDistance then
                            closestDistance=distance
                            targetPlayer=player
                        end
                    end
                end
            end
        end
    end
    
    return targetPlayer
end

