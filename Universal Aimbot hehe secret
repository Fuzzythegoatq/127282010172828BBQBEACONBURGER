local function v361(v398)
    local v399=v398.Size;
    v398.MouseEnter:Connect(function()
        v3:Create(v398,TweenInfo.new(0.2),{Size=v399+UDim2.new(0,5,0,5)}):Play();
        v398.BackgroundColor3=Color3.new(0.25,0.25,0.25);
    end);
    v398.MouseLeave:Connect(function()
        v3:Create(v398,TweenInfo.new(0.2),{Size=v399}):Play();
        v398.BackgroundColor3=Color3.new(0.15,0.15,0.15);
    end);
end
v361(v99);
v361(v113);
v361(v73);
local v362="ESP";
local function v363(v400)
    v362=v400;
    v127.Visible=v400=="ESP";
    v135.Visible=v400=="Aimbot";
    local v402={v99,v113};
    for _,v520 in ipairs(v402) do
